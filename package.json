{
  "name": "docsify-starter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "",
    "dev": "yarn docsify serve docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
